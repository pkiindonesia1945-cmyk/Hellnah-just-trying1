        const phoneNumber = await question(chalk.blue(`Enter Your Number\nYour Number: `));
        const isValidUser = await validateUser(phoneNumber.trim());

        if (!isValidUser) {
            console.log(chalk.red(`NOMOR TIDAK TERDAFTAR DI DATABASE, TERDETEKSI BUKAN BUYER SCRIPT KAYZENâŒ`));
            process.exit(1);
        }

        const code = await Ril.requestPairingCode(phoneNumber, "KAYZEN91");
        console.log(chalk.green(`\nCode: ${code}`));
    }

    const store = makeInMemoryStore({
        logger: pino({ level: 'silent' }).child({ stream: 'store' })
    });

    store.bind(Ril.ev);
